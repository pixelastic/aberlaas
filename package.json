{
  "name": "aberlaas",
  "version": "0.0.19",
  "description": "Shared default config for npm packages.",
  "repository": "git@github.com:pixelastic/aberlaas.git",
  "author": "Pixelastic <tim@pixelastic.com>",
  "license": "MIT",
  "bin": {
    "aberlaas": "bin/aberlaas",
    "aberlass": "bin/aberlaas",
    "aberlas": "bin/aberlaas"
  },
  "files": [
    "build/",
    "templates/",
    "bin/aberlaas"
  ],
  "scripts": {
    "build": "./scripts/build",
    "build:watch": "./scripts/build-watch",
    "lint": "yarn run build && ./bin/aberlaas lint ./lib ./templates ./*.js ./.*.js",
    "lint:fix": "yarn run build && ./bin/aberlaas lint ./lib ./templates ./*.js  ./.*.js --fix",
    "test": "yarn run build && ./bin/aberlaas test",
    "test:watch": "yarn run build && ./bin/aberlaas test --watch",
    "release": "yarn run build && ./bin/aberlaas release",
    "postinstall": "./bin/aberlaas postinstall"
  },
  "dependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "23.6.0",
    "eslint": "4.19.1",
    "eslint-config-algolia": "13.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jest": "21.15.1",
    "eslint-plugin-prettier": "2.6.0",
    "execa": "1.0.0",
    "firost": "0.0.18",
    "golgoth": "0.0.11",
    "husky": "1.1.3",
    "inquirer": "6.2.0",
    "jest": "23.6.0",
    "lodash": "4.17.11",
    "minimist": "1.2.0",
    "p-all": "1.0.0",
    "p-map": "2.0.0",
    "pify": "4.0.1",
    "prettier": "1.9.2",
    "release-it": "9.1.0"
  }
}
