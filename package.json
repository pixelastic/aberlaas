{
  "name": "aberlaas",
  "version": "0.0.39",
  "description": "Shared default config for npm packages.",
  "repository": "git@github.com:pixelastic/aberlaas.git",
  "author": "Pixelastic <tim@pixelastic.com>",
  "license": "MIT",
  "bin": {
    "aberlaas": "bin/aberlaas",
    "aberlass": "bin/aberlaas",
    "aberlas": "bin/aberlaas"
  },
  "files": [
    "build/",
    "templates/",
    "bin/aberlaas"
  ],
  "scripts": {
    "build": "./scripts/build",
    "build:watch": "./scripts/build-watch",
    "lint": "yarn run build && ./bin/aberlaas lint",
    "lint:fix": "yarn run build && ./bin/aberlaas lint ./lib ./templates ./*.js  ./.*.js --fix",
    "test": "yarn run build && ./bin/aberlaas test",
    "test:watch": "yarn run build && ./bin/aberlaas test --watch",
    "release": "yarn run build && ./bin/aberlaas release"
  },
  "dependencies": {
    "@babel/cli": "~7.4.0",
    "@babel/core": "~7.4.0",
    "@babel/node": "~7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "~7.4.0",
    "@babel/preset-env": "~7.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "~10.0",
    "babel-jest": "~24.8.0",
    "eslint": "~5.16.0",
    "eslint-config-prettier": "~4.3.0",
    "eslint-plugin-import": "~2.17.0",
    "eslint-plugin-jest": "~22.6.0",
    "eslint-plugin-jsdoc": "~8.3.0",
    "eslint-plugin-prettier": "~3.1.0",
    "execa": "1.0.0",
    "firost": "~0.1.0",
    "golgoth": "~0.1.0",
    "husky": "1.1.3",
    "inquirer": "6.2.0",
    "jest": "~24.8.0",
    "minimist": "1.2.0",
    "prettier": "~1.18.0",
    "release-it": "~12.3.0"
  }
}
